<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StudentTestingApp.View.TestNavigationPage"
            xmlns:local="clr-namespace:StudentTestingApp.View"
            BarBackgroundColor="#212121"
            ItemsSource="{Binding QuestionViews}">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="{Binding RemainingSeconds}"/>
        <ToolbarItem Icon="{Binding ClockIconName}"/>
        <ToolbarItem Icon="ok.png" Clicked="OkClicked"/>
    </TabbedPage.ToolbarItems>
</TabbedPage>