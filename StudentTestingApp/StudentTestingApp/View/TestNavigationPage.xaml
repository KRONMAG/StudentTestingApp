<?xml version="1.0" encoding="utf-8" ?>

<TabbedPage
    x:Class="StudentTestingApp.View.TestNavigationPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Тестирование"
    BarBackgroundColor="#212121">
    <TabbedPage.ToolbarItems>
        <ToolbarItem x:Name="RemainingSecondsToolbarItem" />
        <ToolbarItem x:Name="ClockToolbarItem" IconImageSource="lock_clock.png" />
        <ToolbarItem Clicked="FinishTestClicked" Icon="ok.png" />
    </TabbedPage.ToolbarItems>
</TabbedPage>